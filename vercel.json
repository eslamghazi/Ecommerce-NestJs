{
    "version": 2,
    "builds": [
        {
            "src":"src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest":"src/main.ts",
            "methods": ["GET", "POST","PUT", "DELETE"]
        },
        {
      "src": "/swagger(.*)",
      "dest": "dist/main.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    }
    ]
}